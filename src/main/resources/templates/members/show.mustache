{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>
    {{#member}}
        <tr>
            <th>{{id}}</th>
            <td>{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/member}}
    </tbody>
</table>
<a href="/members" class="btn btn-primary">유저 목록</a>
<a href="/members/{{member.id}}/edit" class="btn btn-warning">수정</a>
<a href="/members/{{member.id}}/edit" class="btn btn-danger">삭제<a>

{{>layouts/footer}}